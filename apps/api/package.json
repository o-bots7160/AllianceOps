  "devDependencies": {
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "prisma": "^6.2.0",
    "typescript": "^5.7.0",
    "vitest": "^2.1.0"
  }
}
